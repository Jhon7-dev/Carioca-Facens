package main;

import entities.Pilha;

public class TestPilha {

	public static void main(String[] args) {
		Pilha pilha = new Pilha(10);
		pilha.empilhar(10);
		pilha.empilhar(20);
		pilha.empilhar(30);
		pilha.empilhar(40);
		pilha.desempilhar();
		pilha.empilhar(50);
		pilha.imprimirPilha();
	}

}
