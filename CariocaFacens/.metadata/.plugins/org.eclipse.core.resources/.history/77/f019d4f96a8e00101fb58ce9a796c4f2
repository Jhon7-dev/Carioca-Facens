package entities;

import java.util.ArrayList;
import java.util.List;

public class PilhaList {
	private List <Object>itens = new ArrayList<>();
	public PilhaList() {
		// TODO Auto-generated constructor stub
	}
	public PilhaList(List<Object> itens) {
		this.itens = itens;
	}
	public List<Object> getItens() {
		return itens;
	}

	public void empilhar(Object item) {
		itens.add(item);
	}
	public void desempilhar(Object item) {
		itens.remove(item);
	}
	public void tamanhoPilha() {
		itens.size();
	}
	//public void pilhaVazia() {
