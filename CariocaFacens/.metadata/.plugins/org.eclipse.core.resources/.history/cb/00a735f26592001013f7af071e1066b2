package com.joao.estruturaDeDados.application;

import com.joao.estruturaDeDados.entities.StackAsArray;

public class ProgarmaStack {

	public static void main(String[] args) {
		StackAsArray pilha = new StackAsArray(10);
		
		pilha.push("J");
		pilha.push("o");
		pilha.push("Ã£");
		pilha.push("o");
		pilha.push("v");
		pilha.push("i");
		pilha.push("c");
		pilha.push("t");
		pilha.push("o");
		pilha.push("r");
		System.out.println("Topo: " + pilha.getTop());
		Object removido = pilha.pop();
		System.out.println("Elemento removido: " + removido);
		//pilha.toString();
		pilha.getCount();
		System.out.println(pilha);

	}

}
