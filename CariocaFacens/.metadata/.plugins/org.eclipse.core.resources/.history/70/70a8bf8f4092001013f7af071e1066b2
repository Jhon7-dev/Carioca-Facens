package com.joao.estruturaDeDados.application;

import com.joao.estruturaDeDados.entities.Array;

public class Program {

	public static void main(String[] args) {
	
		Array a1 = new Array(3, 0);
		a1.put(0, "a");
		a1.put(1, "v");
		a1.put(2, "c");
		
		System.out.println("a1: " + a1 );
		
		Array a2 = new Array(3,0);
		
		a2.assign(a1);
		
		System.out.println("a2 assign --> " + a2);
	}

}
